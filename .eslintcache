n  const [genres, setGenres] = useState([]);\n  useEffect(async () => {\n    const fetchMovies = async () => {\n      const { data: { results } } = await fetch.get(moviesUrl);\n\n      return results.map(({ id, poster_path, genre_ids }) => ({\n        id,\n        imageUrl: imageBaseUrl + poster_path,\n        genreIds: genre_ids,\n      }));\n    };\n    try {\n      const data = await fetchMovies();\n      setMovies(data);\n      setGenres(data.map(({ genreIds }) => genreIds).flat());\n    } catch (error) {\n      console.log('Error occurred');\n      console.log(error);\n    }\n  }, []);\n\n  const handleFilter = e => {\n    const section = e.target.closest('section');\n    if (id === section.id) {\n      moviesFilter(e.target.value);\n      setParentsection(section);\n    }\n  };\n\n  const setMoviesToDisplay = parent => {\n    if (parent.id === id) return filterMovies(movies, filterParam);\n    return movies;\n  };\n\n  const allMovies = setMoviesToDisplay(parentSection);\n\n  return (\n    <div className=\"container\">\n      <section id={id}>\n        <Grid className={classes.heading} container justify=\"space-between\" alignItems=\"center\">\n          <Grid item>\n            <h5 className=\"section__heading\">{title}</h5>\n          </Grid>\n          <Grid item>\n            <Filter handleFilter={handleFilter} genres={genres} />\n          </Grid>\n        </Grid>\n        <Grid className={`${classes.container} scrollbar`} container spacing={3}>\n          {allMovies?.map(({ id, imageUrl }) => (\n            <Grid className={classes.itemRoot} item key={id}>\n              <MovieCard imgUrl={imageUrl} />\n            </Grid>\n          ))}\n        </Grid>\n      </section>\n    </div>\n  );\n}\n\nMovieRow.propTypes = {\n  title: PropTypes.string.isRequired,\n  moviesUrl: PropTypes.string.isRequired,\n  filterParam: PropTypes.string.isRequired,\n  moviesFilter: PropTypes.func.isRequired,\n  id: PropTypes.string.isRequired,\n};\n\nconst mapStateToProps = state => ({\n  filterParam: state.filter,\n});\n\nconst mapDispatchToProps = dispatch => ({\n  moviesFilter: param => dispatch(filterAction(param)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(MovieRow);\n","/home/wales/Desktop/microverse/react/movieflix/src/pages/HomePage.js",["79"],"/home/wales/Desktop/microverse/react/movieflix/src/helpers/data.js",[],"/home/wales/Desktop/microverse/react/movieflix/src/helpers/constants.js",[],"/home/wales/Desktop/microverse/react/movieflix/src/helpers/common.js",[],"/home/wales/Desktop/microverse/react/movieflix/src/components/Filter.js",["80"],"/home/wales/Desktop/microverse/react/movieflix/src/reducers/filter.js",[],"/home/wales/Desktop/microverse/react/movieflix/src/actions/type.js",[],"/home/wales/Desktop/microverse/react/movieflix/src/actions/filter.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":24,"column":7,"nodeType":"97","messageId":"98","endLine":24,"endColumn":18},{"ruleId":"95","severity":1,"message":"96","line":25,"column":7,"nodeType":"97","messageId":"98","endLine":25,"endColumn":18},{"ruleId":"95","severity":1,"message":"96","line":48,"column":7,"nodeType":"97","messageId":"98","endLine":48,"endColumn":18},{"ruleId":"95","severity":1,"message":"96","line":49,"column":7,"nodeType":"97","messageId":"98","endLine":49,"endColumn":18},{"ruleId":"99","severity":2,"message":"100","line":8,"column":1,"nodeType":"101","messageId":"102","endLine":8,"endColumn":5,"fix":"103"},{"ruleId":"95","severity":1,"message":"96","line":5,"column":3,"nodeType":"97","messageId":"98","endLine":5,"endColumn":14},"jsx-a11y/accessible-emoji",[],"lines-around-directive",["104"],"no-spaced-func",["105"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path